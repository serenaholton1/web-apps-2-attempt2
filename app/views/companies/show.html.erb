<!-- Company details page -->

<h1><%= @company["name"] %></h1>

<h2><%= @company["city"] %>, <%= @company["state"] %></h2>

<h3>Contacts</h3>

<ul>
  <% @contacts.each do |contact| %>
    <li><%= contact["last_name"] %>, <%= contact["first_name"] %></li>
  <% end %>
</ul>

<p>
  <a href="">New Contact</a>
</p>

<p>
  <a href="/companies">Back to All Companies</a>
</p>
